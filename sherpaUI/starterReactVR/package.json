{
  "name": "starterReactVR",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron -r babel-register . && node node_modules/react-native/local-cli/cli.js start && npm run start2",
    "start2": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "bundle": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "devtools": "react-devtools",
    "test": "jest",
    "publish": "npm run bundle",
    "dist": "build",
    "pack": "build --dir"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "child_process": "^1.0.2",
    "cors": "^2.8.3",
    "electron-default-menu": "^1.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "^3.0.1",
    "https-browserify": "0.0.1",
    "local-storage": "^1.4.2",
    "node-dom": "^0.1.0",
    "node-localstorage": "^1.3.0",
    "os-homedir": "^1.0.2",
    "ovrui": "^1.2.0",
    "path": "^0.12.7",
    "react": "~15.4.1",
    "react-desktop": "^0.3.0",
    "react-dom": "^15.3.2",
    "react-native": "~0.42.0",
    "react-native-crypto": "^2.0.2",
    "react-native-fs": "^2.3.3",
    "react-native-level-fs": "^3.0.0",
    "react-native-randombytes": "^2.2.0",
    "react-vr": "^1.2.0",
    "react-vr-web": "^1.2.0",
    "three": "^0.80.1"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "jest": "^19.0.2",
    "react-devtools": "^2.1.3",
    "react-test-renderer": "~15.4.1",
    "xopen": "1.0.0",
    "ajv": "^5.1.6",
    "electron": "^1.4.3"
  },
  "jest": {
    "preset": "react-vr"
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "https": "https-browserify",
    "fs": "react-native-level-fs"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "https": "https-browserify",
    "fs": "react-native-level-fs"
  },
  "build": {
    "appId": "SherpaUI",
    "mac": {
      "category": "public.app-category.graphics-design",
      "icon": "./build/icon.icns"
    }
  }
}
